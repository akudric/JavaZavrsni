<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hr.java.zavrsni.entities.Controllers.StartController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="321.0" prefWidth="300.0" VBox.vgrow="ALWAYS">
      <children>
            <TextField fx:id="loadData" layoutX="230.0" layoutY="109.0" />
            <Text layoutX="176.0" layoutY="80.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Ime datoteke za ucitavanje imena igraca te teme" />
            <Button fx:id="loadButton" layoutX="278.0" layoutY="178.0" mnemonicParsing="false" text="Load" />
            <TextField fx:id="player1Name" layoutX="81.0" layoutY="244.0" />
            <TextField fx:id="player2Name" layoutX="81.0" layoutY="286.0" />
            <TextField fx:id="theme" layoutX="81.0" layoutY="332.0" />
            <Text layoutX="278.0" layoutY="261.0" strokeType="OUTSIDE" strokeWidth="0.0" text="White" />
            <Text layoutX="280.0" layoutY="303.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Black" />
            <Text layoutX="276.0" layoutY="349.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Theme" />
            <Button fx:id="startGameButton" layoutX="422.0" layoutY="237.0" mnemonicParsing="false" prefHeight="123.0" prefWidth="149.0" text="Start" />
            <MenuBar layoutY="2.0">
              <menus>
                <Menu mnemonicParsing="false" text="Home">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Home" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Games">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#openPretrazivanjeIgri" text="Search" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Players">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#openPretrazivanjeIgraca" text="Search" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
      </children>
    </AnchorPane>
  </children>
</VBox>
